{
  "name": "circleci-cli",
  "viewName": "CircleCI CLI",
  "type": "executer",
  "main": "app.js",
  "execProgram": "node",
  "exposeRoute": false,
  "imgUrl": "logo.png",
  "version": "0.0.1",
  "description": "This is CircleCI Kaholo Plugin.",
  "category": [
    "CI/CD"
  ],
  "keywords": [
    "circleci",
    "plugin",
    "kaholo",
    "ci"
   ],
  "methods": [
    {
      "name": "executeJob",
      "viewName": "Execute Job",
      "params": [
        {
          "name": "workingDirectory",
          "viewName": "Working directory",
          "type": "string",
          "required": true,
          "description": "Directory containing Circle CI pipeline configuration, .circleci/config.yml."
        },
        {
          "name": "jobName",
          "viewName": "Job name",
          "type": "string",
          "required": true,
          "default": "build",
          "description": "Name of the job to run"
        }
      ]
    }
  ]
}
